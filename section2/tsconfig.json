{
    "compilerOptions": {
        "target": "ESNext",
        "module": "ESNext",
        "outDir": "dist",
        "strict": true,
        "strictNullChecks": false,
        "moduleResolution": "node",
        "baseUrl": ".",
        "paths": {
            "undici-types": ["node_modules/undici-types"]
        }
    },
    "ts-node": {
        "compilerOptions": {
            "module": "commonjs",
            "esModuleInterop": true
        },
        "files": true
    },
    "include": ["src"]
}
